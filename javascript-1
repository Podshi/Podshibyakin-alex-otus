"use strict";

function Sum(arg) {
    let currentSum = 0;
    if (arg != null) {
        currentSum = arg;
    } 

    function f(b){
        if (b != null){
            currentSum += b;
        }
        return f;
    }
   
    f.toString = function(){
        return currentSum;
    }
   
    return f;
    };
  console.log ( Sum(5)(2)(3)());
